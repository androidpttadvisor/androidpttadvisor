<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gov.cdc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="14" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="gov.cdc.MainView"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="gov.cdc.ActivityEins"
            android:label="activity_eins" />
        <activity
            android:name="gov.cdc.RestartDialog"
            android:label="restartdialog" />
        <activity
            android:name="gov.cdc.InfoView"
            android:label="@string/title_activity_info_view" />
        <activity
            android:name="gov.cdc.HistoryView"
            android:label="History" />
        <activity
            android:name="gov.cdc.HelpView"
            android:label="@string/title_activity_help_view" >
        </activity>
        <activity
            android:name="gov.cdc.FootnotesView"
            android:label="@string/title_activity_footnotes_view" >
        </activity>
    </application>

</manifest>